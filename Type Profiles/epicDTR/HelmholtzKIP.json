    "expectedUses": [
      "Describes FAIR digital objects with metadata properties as suggested by the Helmholtz Metadata Collaboration to optimize their usage by machine-actionable client systems."
    ],
    "representationsAndSemantics": [
      {
        "expression": "Format",
        "value": "PROFILE_DEFINITION",
        "subSchemaRelation": "allowAdditionalProperties"
      }
    ],
    "properties": [
      {
        "name": "kernelInformationProfile",
        "identifier": "21.T11148/076759916209e5d62bd5",
        "representationsAndSemantics": [
          {
            "expression": "Format",
            "value": "PROFILE_DEFINITION",
            "obligation": "Mandatory",
            "repeatable": "Yes",
            "details": "Describes the properties a record of this type will have. This will actually be a reference to this profile."
          }
        ]
      },
      {
        "name": "digitalObjectType",
        "identifier": "21.T11148/c83481d4bf467110e7c9",
        "representationsAndSemantics": [
          {
            "expression": "Format",
            "value": "TYPE_DEFINITION",
            "obligation": "Optional",
            "repeatable": "No",
            "details": "Describes the type of the object that is references by such a record."
          }
        ]
      },
      {
        "name": "digitalObjectLocation",
        "identifier": "21.T11148/b8457812905b83046284",
        "representationsAndSemantics": [
          {
            "expression": "Format",
            "value": "TYPE_DEFINITION",
            "obligation": "Mandatory",
            "repeatable": "Yes",
            "details": "A url as location. As a workaround, a PID in proxy notation (resolvers url) can be used instead if needed."
          }
        ]
      },
      {
        "name": "etag",
        "identifier": "21.T11148/92e200311a56800b3e47",
        "representationsAndSemantics": [
          {
            "expression": "Format",
            "value": "TYPE_DEFINITION",
            "obligation": "Optional",
            "repeatable": "No",
            "details": "Problem: Type is not indivisible."
          }
        ]
      },
      {
        "name": "dateCreated",
        "identifier": "21.T11148/aafd5fb4c7222e2d950a",
        "representationsAndSemantics": [
          {
            "expression": "Format",
            "value": "TYPE_DEFINITION",
            "obligation": "Mandatory",
            "repeatable": "No",
            "details": "Date/time of object creation. (context : KernelInformation)"
          }
        ]
      },
      {
        "name": "dateModified",
        "identifier": "21.T11148/397d831aa3a9d18eb52c",
        "representationsAndSemantics": [
          {
            "expression": "Format",
            "value": "TYPE_DEFINITION",
            "obligation": "Mandatory",
            "repeatable": "Yes",
            "details": "This is the same as in the recommended profile. The name is different to indicate that it has the same format as the dateCreated property."
          }
        ]
      },
      {
        "name": "version",
        "identifier": "21.T11148/c692273deb2772da307f",
        "representationsAndSemantics": [
          {
            "expression": "Format",
            "value": "TYPE_DEFINITION",
            "obligation": "Mandatory",
            "repeatable": "No",
            "details": "If tracked, a version for the object, which must follow a total order. Mandatory for all objects with at least one predecessor version. (context : PID_Kernel_Information)"
          }
        ]
      },
      {
        "name": "contact",
        "identifier": "21.T11148/1a73af9e7ae00182733b",
        "representationsAndSemantics": [
          {
            "expression": "Format",
            "value": "TYPE_DEFINITION",
            "obligation": "Mandatory",
            "repeatable": "Yes",
            "details": "A web-resolvable pointer to an institution or a person responsible for the digital object."
          }
        ]
      },
      {
        "name": "license",
        "identifier": "21.T11148/dc54ae4b6807f5887fda",
        "representationsAndSemantics": [
          {
            "expression": "Format",
            "value": "TYPE_DEFINITION",
            "obligation": "Mandatory",
            "repeatable": "No",
            "details": "names license requirement of the collection (type : string, currently, but could be also given as a controlled vocabulary, like '[\"CC0\", \"CC-BY\", \"CC-BY-SA\", \"CC-by-NC\", \"CC-BY-ND\", \"CC-BY-NC-SA\", \"CC-BY-NC-ND\"]', context : collection)."
          }
        ]
      },
      {
        "name": "topic",
        "identifier": "21.T11148/b415e16fbe4ca40f2270",
        "representationsAndSemantics": [
          {
            "expression": "Format",
            "value": "TYPE_DEFINITION",
            "obligation": "Optional",
            "repeatable": "Yes",
            "details": "One or more topics the digital object is related to."
          }
        ]
      },
      {
        "name": "hasMetadata",
        "identifier": "21.T11148/d0773859091aeb451528",
        "representationsAndSemantics": [
          {
            "expression": "Format",
            "value": "TYPE_DEFINITION",
            "obligation": "Optional",
            "repeatable": "Yes",
            "details": "One or more PID(s) referring to related FAIR DOs providing metadata\nfor the digital object."
          }
        ]
      },
      {
        "name": "isMetadataFor",
        "identifier": "21.T11148/4fe7cde52629b61e3b82",
        "representationsAndSemantics": [
          {
            "expression": "Format",
            "value": "TYPE_DEFINITION",
            "obligation": "Optional",
            "repeatable": "Yes",
            "details": "A PID pointing to another FAIR DO describing the digital object."
          }
        ]
      },
      {
        "name": "wasGeneratedBy",
        "identifier": "21.T11148/c085f1292d7d4a338d96",
        "representationsAndSemantics": [
          {
            "expression": "Format",
            "value": "TYPE_DEFINITION",
            "obligation": "Optional",
            "repeatable": "No",
            "details": "A PID pointing to an activity which generated a object."
          }
        ]
      },
      {
        "name": "wasDerivedFrom",
        "identifier": "21.T11148/c6e4c19f294ee6f41b1e",
        "representationsAndSemantics": [
          {
            "expression": "Format",
            "value": "TYPE_DEFINITION",
            "obligation": "Optional",
            "repeatable": "Yes",
            "details": "Transformation of an entity into another, an update of an entity resulting in a new one, or the construction of a new entity based on a pre-existing entity. (context : KernelInformation)"
          }
        ]
      },
      {
        "name": "specializationOf",
        "identifier": "21.T11148/ab53242825e85a0a7f76",
        "representationsAndSemantics": [
          {
            "expression": "Format",
            "value": "TYPE_DEFINITION",
            "obligation": "Optional",
            "repeatable": "Yes",
            "details": "Entity is a specialization of another that shares all aspects of the latter, and additionally presents more specific aspects of the same thing as the latter. (context : KernelInformation)"
          }
        ]
      },
      {
        "name": "wasRevisionOf",
        "identifier": "21.T11148/2a1cad55473b20407c78",
        "representationsAndSemantics": [
          {
            "expression": "Format",
            "value": "TYPE_DEFINITION",
            "obligation": "Optional",
            "repeatable": "Yes",
            "details": "A derivation for which the resulting entity is a revised version of some original. (context : KernelInformation)"
          }
        ]
      },
      {
        "name": "hadPrimarySource",
        "identifier": "21.T11148/a753134738da82809fc1",
        "representationsAndSemantics": [
          {
            "expression": "Format",
            "value": "TYPE_DEFINITION",
            "obligation": "Optional",
            "repeatable": "Yes",
            "details": "A primary source for a topic refers to something produced by some agent with direct experience and knowledge about the topic, at the time of the topic's study, without benefit from hindsight. (context : KernelInformation)"
          }
        ]
      },
      {
        "name": "wasQuotedFrom",
        "identifier": "21.T11148/beaeecebec408908de35",
        "representationsAndSemantics": [
          {
            "expression": "Format",
            "value": "TYPE_DEFINITION",
            "obligation": "Optional",
            "repeatable": "Yes",
            "details": "Used for the repeat of (some or all of) an entity, such as text or image, by someone who may or may not be its original author. (context : KernelInformation)"
          }
        ]
      },
      {
        "name": "alternateOf",
        "identifier": "21.T11148/432132bdbd946b2baf2b",
        "representationsAndSemantics": [
          {
            "expression": "Format",
            "value": "TYPE_DEFINITION",
            "obligation": "Optional",
            "repeatable": "Yes",
            "details": "Entities present aspects of the same thing. These aspects may be the same or different, and the alternate entities may or may not overlap in\ntime. (context : KernelInformation)"
          }
        ]
      },
      {
        "name": "provenanceGraph",
        "identifier": "21.T11148/af11e18f83466642c47d",
        "representationsAndSemantics": [
          {
            "expression": "Format",
            "obligation": "Optional",
            "repeatable": "No",
            "details": "A PID pointing to another FAIR DO which refers to the provenance graph of a object."
          }
        ]
      }
    ]
